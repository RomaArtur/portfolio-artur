generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id                   Int      @id @default(autoincrement())
  name                 String
  bio                  String  
  linkedinUrl          String  
  githubUrl            String 
  whatsappUrl          String 
  curriculumVitaeUrl   String  
}

